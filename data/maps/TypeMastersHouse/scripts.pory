script TeachTutorMove {
    msgbox("Who to teach move to?")
    special(ChooseMonForMoveTutor)
    waitstate
    goto_if_eq(VAR_RESULT, FALSE, TutorExitMoveTeach)
    removemoney(5000)
    msgbox("Thankyou, come back soon!")
    releaseall
    end
}