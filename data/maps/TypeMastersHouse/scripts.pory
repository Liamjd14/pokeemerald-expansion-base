script MovePhysicalTypeSelect {
    lockall
    setvar(VAR_0x8008, 5000)
    checkmoney(5000)
    goto_if_eq(VAR_RESULT, FALSE, TutorCantBuy)
    msgbox("Welcome to the Physical Move Counter,\nWhat move type do you want?")
    dynmultichoice(0,0,FALSE,6,0,FALSE, "Bug", "Dark", "Dragon", "Eletric", "Fairy", "Fighting", "Fire", "Flying", "Ghost", "Grass", "Ground", "Ice", "Normal", "Poison", "Psychic", "Rock", "Steel", "Water")
    switch(var(VAR_RESULT)){
        case 0:
            msgbox("Bug Type Moves Selected.")
            goto(BugPhysical)
        case 1:
            msgbox("Dark Type Moves Selected.")
            goto(DarkPhysical)
        case 2:
            msgbox("Dragon Type Moves Selected.")
            goto(DragonPhysical)
        case 3:
            msgbox("Eletric Type Moves Selected.")
            goto(ElectricPhysical)
        case 4:
            msgbox("Fairy Type Moves Selected.")
            goto(FairyPhysical)
        case 5:
            msgbox("Fighting Type Moves Selected.")
            goto(FightingPhysical)
        case 6:
            msgbox("Fire Type Moves Selected.")
            goto(FirePhysical)
        case 7:
            msgbox("Flying Type Moves Selected.")
            goto(FlyingPhysical)
        case 8:
            msgbox("Ghost Type Moves Selected.")
            goto(GhostPhysical)
        case 9:
            msgbox("Grass Type Moves Selected.")
            goto(GrassPhysical)
        case 10:
            msgbox("Ground Type Moves Selected.")
            goto(GroundPhysical)
        case 11:
            msgbox("Ice Type Moves Selected.")
            goto(IcePhysical)
        case 12:
            msgbox("Normal Type Moves Selected.")
            goto(NormalPhysical)
        case 13:
            msgbox("Poison Type Moves Selected.")
            goto(PoisonPhysical)
        case 14:
            msgbox("Psychic Type Moves Selected.")
            goto(PsychicPhysical)
        case 15:
            msgbox("Rock Type Moves Selected.")
            goto(RockPhysical)
        case 16:
            msgbox("Steel Type Moves Selected.")
            goto(SteelPhysical)
        case 17:
            msgbox("Water Type Moves Selected.")
            goto(WaterPhysical)
        case 127:
            msgbox("Please Come Back Again!")
            releaseall
    }
}

